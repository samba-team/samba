	-DLMHOSTSFILE=\"$(LMHOSTSFILE)\" \
	-DSWATDIR=\"$(SWATDIR)\" \
	-DLOCKDIR=\"$(LOCKDIR)\" \
	-DPIDDIR=\"$(PIDDIR)\" \
	-DLIBDIR=\"$(LIBDIR)\" \
	-DLOGFILEBASE=\"$(LOGFILEBASE)\" \
	-DSHLIBEXT=\"@SHLIBEXT@\" \
	-DCTDBDIR=\"$(CTDBDIR)\" \
	-DCONFIGDIR=\"$(CONFIGDIR)\" \
	-DCODEPAGEDIR=\"$(CODEPAGEDIR)\" \
	-DCACHEDIR=\"$(CACHEDIR)\" \
	-DSTATEDIR=\"$(STATEDIR)\"

