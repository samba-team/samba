  rm -rf conftest*
  ac_cv_header_shadow_h=yes
else
  echo "configure: failed program was:" >&5
  cat conftest.$ac_ext >&5
  rm -rf conftest*
  ac_cv_header_shadow_h=no
fi
rm -f conftest*
		if test x"$ac_cv_header_shadow_h" = x"yes"; then
		   cat >> confdefs.h <<\EOF
#define HAVE_SHADOW_H 1
EOF

		fi
	;;
esac
